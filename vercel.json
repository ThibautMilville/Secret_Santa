{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@now/python",
            "config": { "maxLambdaSize": "50mb", "functions": {"api/**/*.py": { "runtime": "python3.9" }}}
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
}